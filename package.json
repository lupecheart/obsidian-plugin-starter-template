{
	"name": "obsidian-plugin-starter-template",
	"version": "1.3.0",
	"description": "A robust starter template for developing Obsidian plugins with modern tooling and best practices.",
	"main": "main.js",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/luispernia/obsidian-plugin-starter-template.git"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"prepare": "husky",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [],
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^7.1.0",
		"@semantic-release/git": "^10.0.1",
		"@types/node": "^25.0.3",
		"esbuild": "0.25.5",
		"esbuild-plugin-tsconfig-paths": "^1.0.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"husky": "^9.1.7",
		"jiti": "2.6.1",
		"jsdom": "^27.4.0",
		"lint-staged": "^16.2.7",
		"moment": "^2.30.1",
		"prettier": "^3.7.4",
		"semantic-release": "^25.0.2",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"vite-tsconfig-paths": "^6.0.3",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
